#include<math.h>
#include<iostream>
#include<string>
using namespace std;
int main(){
    string str;
    cout<<"Nhập chuỗi:"<<endl;
    getline(std::cin,str);
    cout<<"Các kí tự chẵn trong chuỗi:"<<endl;
    int n;
    n = str.size();
    for(int i=0 ; i<n ; i=i+2){
        cout<<str[i]<<" ";
    }
    return 0;
}
